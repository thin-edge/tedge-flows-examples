{
  "name": "protobuf-xform",
  "version": "0.0.1",
  "description": "This flow translates sensor data being received as json messages, and transforms them as protobuf messages",
  "source": "src/main.ts",
  "module": "dist/main.mjs",
  "type": "module",
  "scripts": {
    "prebuild": "npx buf generate",
    "build": "esbuild src/main.ts --target=es2018 --bundle --outfile=dist/main.mjs --format=esm --drop-labels=DEV,TEST",
    "test": "jest --coverageProvider=v8 --coverage"
  },
  "author": "thin-edge.io",
  "license": "Apache-2.0",
  "devDependencies": {
    "@bufbuild/buf": "^1.60.0",
    "@bufbuild/protoc-gen-es": "^2.10.1",
    "@faker-js/faker": "^10.1.0",
    "@types/jest": "^30.0.0",
    "esbuild": "^0.27.0",
    "esbuild-register": "^3.6.0",
    "jest": "^30.2.0",
    "jest-esbuild": "^0.4.0",
    "prettier": "3.6.2",
    "protobufjs-cli": "^1.1.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.1"
  }
}
